#!/bin/sh

#-------------------------------------------------------------------------------
# remove [RTR/123] messages from `cf log`
#-------------------------------------------------------------------------------

grep --line-buffered --invert-match --fixed-strings "[RTR/"

#-------------------------------------------------------------------------------
#          1         2         3         4         5
# 12345678901234567890123456789012345678901234567890
# --------------------------------------------------------------------------
# 2015-03-05T09:20:00.17-0500 [RTR/0]      OUT <message here>
#-------------------------------------------------------------------------------
